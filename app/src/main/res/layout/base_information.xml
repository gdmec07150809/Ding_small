<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px30"
        android:layout_marginRight="@dimen/px30"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="UUID"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/ssid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="164262164216"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MAC"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/macNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="无"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="品牌名称"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true"
                android:id="@+id/brand_name_text" />

            <TextView
                android:id="@+id/eqpBrand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="YOUPLUS"
                android:layout_alignBaseline="@+id/brand_name_text"
                android:layout_alignBottom="@+id/brand_name_text"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="品类"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true"
                android:id="@+id/category_text" />

            <TextView
                android:id="@+id/eqpStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="吧台灯"
                android:layout_alignBaseline="@+id/category_text"
                android:layout_alignBottom="@+id/category_text"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:id="@+id/selling_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="销售日期"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/startDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="2017-12-30"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px90">
            <TextView
                android:id="@+id/shelf_life_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="保修期"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/protectDateTo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textSize="@dimen/px30"
                android:textColor="@color/list_item_color"
                android:text="12个月"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>